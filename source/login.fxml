      <Label layoutX="36.0" layoutY="192.0" text="Nie masz konta?" />
      <TextField fx:id="txtUsername" layoutX="45.0" layoutY="40.0" prefHeight="25.0" prefWidth="198.0" promptText="nazwa użytkownika" />
      <PasswordField fx:id="txtPassword" layoutX="45.0" layoutY="78.0" prefHeight="25.0" prefWidth="198.0" promptText="hasło" />
   </children>
</AnchorPane>
